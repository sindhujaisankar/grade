# grade
providing grades for kids based on marks that they have got.
#include<stdio.h>
int main()
{
  int mark;
  printf("enter the mark : ");
  scanf("%d",&mark);
  if(mark>85&&mark<100)
  {
    printf("grade A");
  }
  else if(mark>70&&mark<84)
  {
    printf("grade B");
  }
  else if(mark>55&&mark<69)
  {
   printf("grade C");
  }
  else if(mark>40&&mark<54)
  {
    printf("grade D");
  }
  else if(mark<40)
  {
    printf("grade F");
  }
  return 0;
}
   
